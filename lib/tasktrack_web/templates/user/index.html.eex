<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for user <- @users do %>
    <tr>
      <td><%= link user.name, to: Routes.user_path(@conn, :show, user) %></td>


    </tr>
<% end %>
  </tbody>
</table>

<%= if @current_user do %>
<span><%= link "New User", to: Routes.user_path(@conn, :new) %></span>
<% end %>